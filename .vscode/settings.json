{
  "cSpell.words": [
    "atrule",
    "Chainable",
    "colspan",
    "compositionend",
    "compositionstart",
    "contenteditable",
    "contextmenu",
    "CRDT",
    "deletable",
    "dppx",
    "esbenp",
    "eventbus",
    "inputarea",
    "keyof",
    "linebreak",
    "mousedown",
    "mouseup",
    "mousemove",
    "mouseleave",
    "noopener",
    "Parens",
    "prismjs",
    "resizer",
    "richtext",
    "rowmargin",
    "rowspan",
    "srcdoc",
    "TEXTLIKE",
    "trlist",
    "updown",
    "vite",
    "vitepress",
    "Yahei"
